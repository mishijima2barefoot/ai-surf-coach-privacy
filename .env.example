# Example environment variables for development / testing
# Copy this file to .env and fill with real values (do NOT commit real secrets)

# Server / app
PORT=8000
SERVER_URL=https://api.example.com
SECRET_KEY=replace-with-a-secure-random-value

# API keys
COACH_API_KEY=replace-with-api-key
STRIPE_API_KEY=replace-with-stripe-key

# Storage / uploads
STORAGE_PROVIDER=s3
S3_BUCKET=your-bucket-name
S3_REGION=us-west-2
S3_ACCESS_KEY_ID=replace-with-access-key
S3_SECRET_ACCESS_KEY=replace-with-secret

# Database (example)
DATABASE_URL=postgresql://user:password@localhost:5432/ai_surf

# Telemetry & privacy
TELEMETRY_OPT_IN=false   # set to true to enable opt-in telemetry (default: false)
PRIVACY_MODE=balanced    # options: strict, balanced

# Logging
LOG_LEVEL=info

# iOS-specific (if applicable)
IOS_APP_BUNDLE_ID=com.example.aisurfcoach

# Android-specific (if applicable)
ANDROID_APPLICATION_ID=com.example.aisurfcoach

# Optional: local-only test values
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=password

# Notes:
# - Never commit real secret keys to the repository. Use a secret manager for CI and production.
# - Adjust variable names to match the app's actual configuration.
